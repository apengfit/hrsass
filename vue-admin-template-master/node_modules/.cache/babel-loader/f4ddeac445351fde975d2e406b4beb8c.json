) => new Router({\n  // mode: 'history', // require service support\n  scrollBehavior: () => ({ y: 0 }),\n  routes: [...constantRoutes, ...asyncRoutes]\n})\n\nconst router = createRouter()\n\n// Detail see: https://github.com/vuejs/vue-router/issues/1234#issuecomment-357941465\nexport function resetRouter() {\n  const newRouter = createRouter()\n  router.matcher = newRouter.matcher // reset router\n}\n\nexport default router\n"],"mappings":";;;;;AAAA,OAAOA,GAAG,MAAM,KAAK;AACrB,OAAOC,MAAM,MAAM,YAAY;AAE/BD,GAAG,CAACE,GAAG,CAACD,MAAM,CAAC;;AAEf;AACA,OAAOE,MAAM,MAAM,UAAU;AAC7B;AACA,OAAOC,eAAe,MAAM,qBAAqB;AACjD,OAAOC,iBAAiB,MAAM,uBAAuB;AACrD,OAAOC,eAAe,MAAM,qBAAqB;AACjD,OAAOC,gBAAgB,MAAM,sBAAsB;AACnD,OAAOC,iBAAiB,MAAM,uBAAuB;AACrD,OAAOC,aAAa,MAAM,mBAAmB;AAC7C,OAAOC,aAAa,MAAM,mBAAmB;AAC7C,OAAOC,YAAY,MAAM,kBAAkB;;AAE3C;AACA,OAAO,IAAMC,WAAW,GAAG,CACzBP,iBAAiB,EACjBK,aAAa,EACbJ,eAAe,EACfC,gBAAgB,EAChBH,eAAe,EACfI,iBAAiB,EACjBC,aAAa,EACbE,YAAY,CACb;AACD;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA,OAAO,IAAME,cAAc,GAAG,CAC5B;EACEC,IAAI,EAAE,QAAQ;EACdC,SAAS,EAAE,SAAAA,UAAA;IAAA,OAAAC,OAAA,CAAAC,OAAA,GAAAC,IAAA;MAAA,OAAAC,uBAAA,CAAAC,OAAA,CAAa,qBAAqB;IAAA;EAAA,CAAC;EAC9CC,MAAM,EAAE;AACV,CAAC,EAED;EACEP,IAAI,EAAE,MAAM;EACZC,SAAS,EAAE,SAAAA,UAAA;IAAA,OAAAC,OAAA,CAAAC,OAAA,GAAAC,IAAA;MAAA,OAAAC,uBAAA,CAAAC,OAAA,CAAa,aAAa;IAAA;EAAA,CAAC;EACtCC,MAAM,EAAE;AACV,CAAC,EAED;EACEP,IAAI,EAAE,GAAG;EACTC,SAAS,EAAEZ,MAAM;EACjBmB,QAAQ,EAAE,YAAY;EACtBC,QAAQ,EAAE,CAAC;IACTT,IAAI,EAAE,WAAW;IACjBU,IAAI,EAAE,WAAW;IACjBT,SAAS,EAAE,SAAAA,UAAA;MAAA,OAAAC,OAAA,CAAAC,OAAA,GAAAC,IAAA;QAAA,OAAAC,uBAAA,CAAAC,OAAA,CAAa,yBAAyB;MAAA;IAAA,CAAC;IAClDK,IAAI,EAAE;MAAEC,KAAK,EAAE,IAAI;MAAEC,IAAI,EAAE;IAAY;EACzC,CAAC;AACH,CAAC;AAED;AACA;EAAEb,IAAI,EAAE,GAAG;EAAEQ,QAAQ,EAAE,MAAM;EAAED,MAAM,EAAE;AAAK,CAAC,CAC9C;AAED,IAAMO,YAAY,GAAG,SAAfA,YAAYA,CAAA;EAAA,OAAS,IAAI3B,MAAM,CAAC;IACpC;IACA4B,cAAc,EAAE,SAAAA,eAAA;MAAA,OAAO;QAAEC,CAAC,EAAE;MAAE,CAAC;IAAA